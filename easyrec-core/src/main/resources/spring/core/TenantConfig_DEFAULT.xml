<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="tenantConfig_default" class="org.easyrec.model.core.TenantConfigVO">
        <constructor-arg index="0" name="itemTypes">
            <list>
                <value>ITEM</value>
            </list>
        </constructor-arg>
        <constructor-arg index="1" name="assocTypes">
            <list>
                <value>VIEWED_TOGETHER</value>
                <value>GOOD_RATED_TOGETHER</value>
                <value>BOUGHT_TOGETHER</value>
                <value>IS_RELATED</value>
                <value>PROFILE_SIMILARITY</value>
            </list>
        </constructor-arg>
        <constructor-arg index="2" name="aggregateTypes">
            <list>
                <value>AVERAGE</value>
                <value>FIRST</value>
                <value>MAXIMUM</value>
                <value>MOST_FREQUENT</value>
                <value>NEWEST</value>
                <value>OLDEST</value>
            </list>
        </constructor-arg>
        <constructor-arg index="3" name="actionTypes">
            <list>
                <value>VIEW</value>
                <value>RATE</value>
                <value>BUY</value>
            </list>
        </constructor-arg>
        <constructor-arg index="4" name="sourceTypes">
            <list>
                <value>MANUALLY_CREATED</value>
            </list>
        </constructor-arg>
        <constructor-arg index="5" name="viewTypes">
            <list>
                <value>ADMIN</value>
                <value>COMMUNITY</value>
                <value>SYSTEM</value>
            </list>
        </constructor-arg>
        <constructor-arg index="6" name="authenticationDomains">
            <list/>
        </constructor-arg>
    </bean>
</beans>